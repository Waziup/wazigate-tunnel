{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {
                "WAZIUP_ADDR": ":8080"
            },
            "args": [
                "-www",
                "${workspaceFolder}/www"
            ]
        }, {
            "name": "Debug",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {
                "WAZIUP_LOG": "debug,date,time"
            },
            "args": [
                "-www",
                "${workspaceFolder}/www"
            ]
        }
    ]
}